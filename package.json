{
  "private": true,
  "engines": {
    "node": "16.x"
  },
  "keywords": [
    "Flow"
  ],
  "author": "Tang BoHao<tech@btang.cn>",
  "license": "MIT",
  "scripts": {
    "start": "node .output/server/index.mjs",
    "dev": "nuxi dev",
    "build": "nuxi build",
    "generate": "nuxi generate",
    "preview": "nuxi preview",
    "lint": "eslint --ext .ts,.vue src && stylelint **/*.vue",
    "update:emulator": "npx dotenv-cli -e .env.emulator -- flow project deploy --update",
    "update:testnet": "npx dotenv-cli -e .env.testnet -- flow project deploy --network=testnet -f flow.json -f flow.testnet.json --update",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@esbuild-plugins/node-globals-polyfill": "^0.1.1",
    "@nuxtjs/tailwindcss": "^5.0.4",
    "@tailwindcss/typography": "^0.5.2",
    "@types/node": "^17.0.31",
    "@typescript-eslint/eslint-plugin": "^5.24.0",
    "@typescript-eslint/parser": "^5.24.0",
    "dotenv-cli": "^5.1.0",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-nuxt": "^3.2.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.7.1",
    "husky": "^8.0.1",
    "lint-staged": "^12.4.1",
    "nuxt": "^3.0.0-rc.3",
    "postcss-html": "^1.4.1",
    "prettier": "^2.6.2",
    "stylelint": "^14.8.2",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended-vue": "^1.4.0",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "@headlessui/vue": "^1.6.1",
    "@heroicons/vue": "^1.0.6",
    "@onflow/fcl": "latest",
    "@onflow/types": "^1.0.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.0"
  }
}
