{
  "private": true,
  "engines": {
    "node": "16.x"
  },
  "keywords": [
    "Flow"
  ],
  "author": "Tang BoHao<tech@btang.cn>",
  "license": "MIT",
  "scripts": {
    "dev": "nuxi dev",
    "build": "nuxi build",
    "preview": "nuxi preview",
    "generate": "nuxi generate",
    "postinstall": "nuxi prepare",
    "prepare": "husky install",
    "lint": "eslint --ext .ts,.vue src && stylelint **/*.vue",
    "update:emulator": "npx dotenv-cli -e .env.emulator -- flow project deploy --update",
    "update:testnet": "npx dotenv-cli -e .env.testnet -- flow project deploy --network=testnet -f flow.json -f flow.testnet.json --update"
  },
  "devDependencies": {
    "@nuxtjs/tailwindcss": "^6.1.3",
    "@tailwindcss/typography": "^0.5.2",
    "@types/node": "^17.0.31",
    "@typescript-eslint/eslint-plugin": "^5.24.0",
    "@typescript-eslint/parser": "^5.24.0",
    "@vueuse/core": "^8.7.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^6.0.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.6.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "nuxt": "^3.0.0",
    "postcss-custom-properties": "^13.0.0",
    "postcss-html": "^1.5.0",
    "prettier": "^2.7.1",
    "stylelint": "^14.8.2",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended-vue": "^1.4.0",
    "typescript": "^4.7.3"
  },
  "dependencies": {
    "@heroicons/vue": "^2.0.13",
    "@onflow/fcl": "^1.3.2",
    "@onflow/types": "^1.0.5"
  }
}
